<h2>Top</h2>
<% 
      polls.sort(function(a,b) {
        return parseInt(a.votes) - parseInt(b.votes);
      });
%>

<% for (var i=0; i<polls.length; i++) { %>
  <h3><a href="/poll/<%= polls[i].id %>"><%= polls[i].title %></a><small>&nbsp;&nbsp;&nbsp; Votes: <%= polls[i].votes %></small></h3> 
<% } %>


